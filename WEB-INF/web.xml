<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>
    <servlet>
        <servlet-name>RoundMatch</servlet-name>
        <servlet-class>servlet.servletMatch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RoundMatch</servlet-name>
        <url-pattern>/match</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Standing</servlet-name>
        <servlet-class>servlet.servletClassifica</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Standing</servlet-name>
        <url-pattern>/standing</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Competition</servlet-name>
        <servlet-class>servlet.servletCompetition</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Competition</servlet-name>
        <url-pattern>/competition</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>servlet.servletCurrentMatch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Team</servlet-name>
        <servlet-class>servlet.servletTeam</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Team</servlet-name>
        <url-pattern>/team</url-pattern>
    </servlet-mapping>
</web-app>